           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Item">
        <property name="merchantReference" type="string"/>
        <property name="offer" type="Offer"/>
    </transfer>

    <transfer name="SalesOrderMerchant">
        <property name="idSalesOrderMerchant" type="int"/>
        <property name="salesOrderMerchantReference" type="string"/>
        <property name="merchantReference" type="string"/>
        <property name="fkSalesOrder" type="int"/>
    </transfer>

    <transfer name="SalesOrderMerchantSave">
        <property name="offerReference" type="string"/>
        <property name="idSalesOrder" type="int"/>
    </transfer>

    <transfer name="Offer">
        <property name="productOfferReference" type="string"/>
    </transfer>

    <transfer name="Quote">
    </transfer>

</transfers>
