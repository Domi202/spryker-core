<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="SynchronizationData">
        <property name="reference" type="string"/>
        <property name="locale" type="string"/>
    </transfer>

    <transfer name="GlossaryStorage">
        <property name="value" type="string"/>
        <property name="glossaryKey" type="GlossaryStorageKey"/>
        <property name="locale" type="GlossaryStorageLocale"/>
    </transfer>

    <transfer name="GlossaryStorageKey">
        <property name="idGlossaryKey" type="int"/>
        <property name="isActive" type="bool"/>
        <property name="key" type="string"/>
    </transfer>

    <transfer name="GlossaryStorageLocale">
        <property name="idLocale" type="int"/>
        <property name="isActive" type="bool"/>
        <property name="localeName" type="string"/>
    </transfer>

</transfers>
