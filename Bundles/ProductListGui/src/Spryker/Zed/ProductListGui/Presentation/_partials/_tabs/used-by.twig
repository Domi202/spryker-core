<table class="table table-striped table-bordered table-hover gui-table-data">
    <thead>
        <tr>
            <td>Title</td>
            <td>Name</td>
            <td>Actions</td>
        </tr>
    </thead>
    <tbody>
        {% for productListUsedByTableRow in productListUsedByTableData.rows %}
            <tr>
                <td>{{ productListUsedByTableRow.entityTitle }}</td>
                <td>{{ productListUsedByTableRow.entityName }}</td>
                <td>{% include '@ProductListGui/_partials/button-collection.twig' with {
                        buttonCollection: productListUsedByTableRow.actionButtons
                    } only %} </td>
            </tr>
        {% endfor %}
    </tbody>
</table>
