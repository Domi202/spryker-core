{
  "name": "spryker/product-offer-gui-page",
  "type": "library",
  "description": "ProductOfferGuiPage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/category": "^4.6.0",
    "spryker/kernel": "^3.30.0",
    "spryker/locale": "^3.0.0",
    "spryker/merchant-user": "^0.2.0",
    "spryker/product": "^6.18.0",
    "spryker/product-category": "^4.7.0",
    "spryker/product-image": "^3.10.0",
    "spryker/product-offer": "^0.3.0",
    "spryker/product-validity": "^1.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/store": "^1.1.0",
    "spryker/symfony": "^3.0.0",
    "spryker/transfer": "^3.18.0",
    "spryker/user": "^3.0.0",
    "spryker/util-encoding": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
