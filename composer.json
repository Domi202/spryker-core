{
  "name": "spryker/spryker",
  "description": "Spryker Framework",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "phpdocumentor/graphviz": "^1.0.4",
    "everon/criteria-builder": "^1.1.5",
    "moneyphp/money": "^3.0.0",
    "fzaninotto/faker": "^1.6"
  },
  "require-dev": {
    "spryker/code-sniffer": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "use-include-path": true,
    "process-timeout": 1200
  },
  "autoload": {
    "files": [
      "autoload.php",
      "Bundles/Config/src/Spryker/Shared/Config.php",
      "Bundles/Development/tests/dd.php"
    ]
  },
  "bin": [
    "Bundles/Console/bin/console"
  ],
  "scripts": {
    "cs-check": "vendor/bin/phpcs Bundles --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -p"
  }
}
